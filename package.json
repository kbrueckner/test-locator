{
  "name": "test-locator",
  "version": "0.1.0",
  "description": "Util to help creating UI test locators.",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "lint": "eslint . --cache --ext .ts",
    "build": "rm -r build || true && tsc -p tsconfig.build.json",
    "release": "npm run lint && npm run test && npm run build && node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbrueckner/test-locator"
  },
  "keywords": [
    "locator",
    "test"
  ],
  "author": "Kenji Br√ºckner <kenji.brueckner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kbrueckner/test-locator/issues"
  },
  "homepage": "https://github.com/kbrueckner/test-locator",
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "eslint": "^7.27.0",
    "jest": "^27.0.1",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0"
  }
}